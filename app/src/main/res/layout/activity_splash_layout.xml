        android:id="@+id/layout_states"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toTopOf="@id/btn_ok"
        app:layout_constraintEnd_toStartOf="@id/splash_gl_right"
        app:layout_constraintStart_toEndOf="@id/tx_bps_main"
        app:layout_constraintTop_toBottomOf="@id/tx_status_version_diff_state">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/inner_gl_mid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
